<GameData>
	<!-- Reduces ranged attcack of Chariot Archers and replacing units by 1.-->
	<Units>
		<Update>
			<Set RangedCombat="9"/>
			<Where Class="UNITCLASS_CHARIOT_ARCHER"/>
		</Update>
		<Update>
			<Set RangedCombat="10"/>
			<Where Type="UNIT_INDIAN_WARELEPHANT"/>
		</Update>
	</Units>  
</GameData>